[server]
init_file = /etc/mysql/init.sql

[mysqld]
user                    = root
pid-file                = /run/mysqld/mysqld.pid
socket                  = /run/mysqld/mysqld.sock
basedir                 = /usr
datadir                 = /var/lib/mysql
tmpdir                  = /tmp
bind-address            = *
port                    = 3306
general_log             = ON
general_log_file        = /var/log/mysql/general.log